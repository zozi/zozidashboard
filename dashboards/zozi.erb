<script type='text/javascript'>
$(function() {
Dashing.widget_base_dimensions = [370, 340]
Dashing.numColumns = 5
Dashing.gridsterLayout('[{"col":3,"row":2,"size_x":2,"size_y":1},{"col":1,"row":2,"size_x":2,"size_y":1},{"col":5,"row":1,"size_x":1,"size_y":1},{"col":1,"row":1,"size_x":2,"size_y":1},{"col":3,"row":1,"size_x":2,"size_y":1},{"col":5,"row":2,"size_x":1,"size_y":1}]')
});
</script>


<% content_for :title do %>Zozi Ops<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="zozi_aws_cpu" data-view="Rickshawgraph" data-title="CPU Utilization" data-renderer="line" data-legend="true" data-max="100" data-color-scheme="rainbow"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="zozi_aws_memory" data-view="Rickshawgraph" data-title="Memory Utilization" data-renderer="line" data-legend="true" data-max="100" data-color-scheme="rainbow"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="zozi_aws_disk_usage" data-view="ProgressBars" data-title="Disk Usage"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="zozi_rpm_errors" data-view="Rickshawgraph" data-title="Error Count" data-renderer="line" data-colors="white" data-max="10"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="zozi_rpm_throughput" data-view="Rickshawgraph" data-title="Request / min" data-renderer="line" data-colors="white"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="zozi_server_status" data-view="ServerStatus" data-title="Server Health Check"></div>
    </li>
  </ul>
</div>
